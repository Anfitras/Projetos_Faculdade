programa
{
	
	funcao inicio()
	{
		real num1, num2

		escreva("Escreva um número: ")
		leia(num1)
		escreva("Escreva outro número: ")
		leia(num2)

		se (num1 > num2){
			escreva("O maior número é ", num1)
		}
		senao{
			escreva("O maior número é ", num2)
		}
	}
}
